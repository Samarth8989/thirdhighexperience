    <script>
        // Initialize EmailJS
        (function() {
            try {
                if (typeof emailjs !== 'undefined') {
                    // Replace 'YOUR_USER_ID' with your actual EmailJS user ID
                    // To set up EmailJS:
                    // 1. Go to https://www.emailjs.com/
                    // 2. Create a free account
                    // 3. Add an email service (Gmail recommended)
                    // 4. Create an email template
                    // 5. Get your User ID, Service ID, and Template ID
                    // 6. Replace the placeholders below
                    emailjs.init("YOUR_USER_ID");
                    console.log('EmailJS initialized successfully');
                }
            } catch (error) {
                console.log('EmailJS not available, using fallback form handling');
            }
        })();

        // Send email function
        function sendEmail(templateParams, successMessage) {
            try {
                if (typeof emailjs !== 'undefined') {
                    // Replace these with your actual EmailJS IDs:
                    // YOUR_SERVICE_ID: e.g., 'service_abc123'
                    // YOUR_TEMPLATE_ID: e.g., 'template_xyz789'
                    emailjs.send('YOUR_SERVICE_ID', 'YOUR_TEMPLATE_ID', templateParams)
                        .then(function(response) {
                            console.log('Email sent successfully', response);
                            alert(successMessage);
                        })
                        .catch(function(error) {
                            console.error('Email send failed', error);
                            alert(successMessage + ' (Offline mode - please contact us directly)');
                        });
                } else {
                    console.log('EmailJS not available, showing fallback message');
                    alert(successMessage + ' (Please contact us directly for immediate assistance)');
                }
            } catch (error) {
                console.error('Error in sendEmail:', error);
                alert(successMessage);
            }
        }

        // Mobile menu functionality
        function toggleMobileMenu() {
            try {
                const mobileNav = document.getElementById('mobileNav');
                const mobileBtn = document.querySelector('.mobile-menu-btn');
                
                if (mobileNav && mobileBtn) {
                    mobileNav.classList.toggle('active');
                    mobileBtn.classList.toggle('active');
                }
            } catch (error) {
                console.error('Error in toggleMobileMenu:', error);
            }
        }

        function closeMobileMenu() {
            try {
                const mobileNav = document.getElementById('mobileNav');
                const mobileBtn = document.querySelector('.mobile-menu-btn');
                
                if (mobileNav && mobileBtn) {
                    mobileNav.classList.remove('active');
                    mobileBtn.classList.remove('active');
                }
            } catch (error) {
                console.error('Error in closeMobileMenu:', error);
            }
        }

        // Navigation functionality with error handling
        function showSection(sectionId, event) {
            try {
                // Prevent any default behavior
                if (event) {
                    event.preventDefault();
                    event.stopPropagation();
                }
                
                // Hide all sections
                const sections = document.querySelectorAll('.section');
                sections.forEach(section => {
                    section.classList.remove('active');
                });
                
                // Show selected section
                const targetSection = document.getElementById(sectionId);
                if (targetSection) {
                    targetSection.classList.add('active');
                    console.log(`Switched to section: ${sectionId}`);
                } else {
                    console.warn(`Section with id '${sectionId}' not found`);
                    return false;
                }
                
                // Update URL hash safely
                try {
                    if (history.pushState && window.location.protocol !== 'about:') {
                        history.pushState(null, null, `#${sectionId}`);
                    } else {
                        window.location.hash = sectionId;
                    }
                } catch (historyError) {
                    // Fallback: just set the hash directly
                    window.location.hash = sectionId;
                }
                
                // Scroll to top smoothly
                window.scrollTo({
                    top: 0,
                    behavior: 'smooth'
                });
                
                return false; // Prevent any further navigation
            } catch (error) {
                console.error('Error in showSection:', error);
                return false;
            }
        }

        // Registration interest modal with error handling
        function registerInterest(productName) {
            try {
                const productInput = document.getElementById('reg-product');
                const modal = document.getElementById('registrationModal');
                
                if (productInput && modal) {
                    productInput.value = productName;
                    modal.style.display = 'block';
                } else {
                    console.error('Modal elements not found');
                    alert(`Thank you for your interest in "${productName}". Please contact us directly for more information.`);
                }
            } catch (error) {
                console.error('Error in registerInterest:', error);
                alert(`Thank you for your interest in "${productName}". Please contact us directly for more information.`);
            }
        }

        function closeModal() {
            try {
                const modal = document.getElementById('registrationModal');
                if (modal) {
                    modal.style.display = 'none';
                }
            } catch (error) {
                console.error('Error in closeModal:', error);
            }
        }

        function submitRegistration(event) {
            try {
                event.preventDefault();
                
                const name = document.getElementById('reg-name')?.value || '';
                const email = document.getElementById('reg-email')?.value || '';
                const phone = document.getElementById('reg-phone')?.value || '';
                const product = document.getElementById('reg-product')?.value || '';
                const message = document.getElementById('reg-message')?.value || '';
                
                if (!name || !email) {
                    alert('Please fill in your name and email address.');
                    return;
                }
                
                // Prepare email data
                const emailData = {
                    type: 'Product Interest',
                    name: name,
                    email: email,
                    phone: phone,
                    product: product,
                    message: message,
                    timestamp: new Date().toLocaleString()
                };
                
                console.log('Product interest registration:', emailData);
                
                // Send email
                sendEmail(emailData, `Thank you ${name}! Your interest in "${product}" has been registered. We'll contact you soon. 🌟`);
                
                // Clear form and close modal
                if (event.target && typeof event.target.reset === 'function') {
                    event.target.reset();
                }
                closeModal();
            } catch (error) {
                console.error('Error in submitRegistration:', error);
                alert('Thank you for your registration. We have received your information.');
                closeModal();
            }
        }

        // File upload handlers
        function handleGuideUpload(input) {
            if (input.files && input.files[0]) {
                const fileName = input.files[0].name;
                const successMsg = document.getElementById('guide-success');
                successMsg.style.display = 'block';
                successMsg.textContent = `Guide "${fileName}" uploaded successfully! 🌟`;
                
                // Add to guide list
                addNewGuide(fileName);
                
                setTimeout(() => {
                    successMsg.style.display = 'none';
                }, 4000);
            }
        }

        function handleAdminVideoUpload(input) {
            if (input.files && input.files[0]) {
                const fileName = input.files[0].name;
                // Store the file for later use when form is submitted
                input.uploadedFile = input.files[0];
                
                // Update the upload area to show selected file
                const uploadArea = input.closest('.file-upload');
                uploadArea.innerHTML = `
                    <div class="file-upload-icon">✅</div>
                    <p><strong>File Selected: ${fileName}</strong></p>
                    <p style="opacity: 0.7; margin-top: 0.5rem;">Click submit to add to gallery</p>
                `;
            }
        }

        function addNewVideo(event) {
            event.preventDefault();
            
            const title = document.getElementById('video-title').value;
            const description = document.getElementById('video-description').value;
            const duration = document.getElementById('video-duration').value;
            const category = document.getElementById('video-category').value;
            const emoji = document.getElementById('video-emoji').value || '🎬';
            const fileInput = document.getElementById('admin-video-upload');
            
            // Create new video container
            const videoGrid = document.querySelector('#breathe .video-grid');
            const newVideoContainer = document.createElement('div');
            newVideoContainer.className = 'video-container';
            
            // Generate a unique ID for the video
            const videoId = 'video-' + Date.now();
            
            // If a file was uploaded, we could handle it here
            // For now, we'll create a placeholder that can be replaced with actual video
            newVideoContainer.innerHTML = `
                <div class="video-placeholder" onclick="playVideo('${videoId}')">
                    <span>${emoji} ${title}<br><small>${duration} • ${category}</small></span>
                </div>
                <div class="video-info">
                    <h3>${title}</h3>
                    <p>${description}</p>
                </div>
            `;
            
            // Add to the grid
            videoGrid.appendChild(newVideoContainer);
            
            // Store video data
            const videoData = {
                id: videoId,
                title: title,
                description: description,
                duration: duration,
                category: category,
                emoji: emoji,
                timestamp: new Date().toISOString()
            };
            
            console.log('New video added:', videoData);
            
            // Clear form
            event.target.reset();
            
            // Reset file upload area
            const uploadArea = document.querySelector('#admin-video-upload').closest('.file-upload');
            uploadArea.innerHTML = `
                <div class="file-upload-icon">🎥</div>
                <input type="file" id="admin-video-upload" accept="video/*" onchange="handleAdminVideoUpload(this)">
                <p><strong>Click to upload video file</strong></p>
                <p style="opacity: 0.7; margin-top: 0.5rem;">Supported formats: MP4, WebM, MOV</p>
            `;
            
            // Show success message
            const successMsg = document.getElementById('admin-video-success');
            successMsg.style.display = 'block';
            setTimeout(() => {
                successMsg.style.display = 'none';
            }, 4000);
        }

        function addNewGuide(fileName) {
            const guideList = document.getElementById('guide-list');
            const newGuide = document.createElement('div');
            newGuide.className = 'card';
            newGuide.innerHTML = `
                <h3>📖 ${fileName.replace(/\.[^/.]+$/, "")}</h3>
                <p>Recently uploaded guide for your spiritual journey and personal growth.</p>
                <a href="#" class="cta-button">Download Guide</a>
            `;
            guideList.insertBefore(newGuide, guideList.firstChild);
        }

        // Video player
        function playVideo(videoId) {
            const videoTitles = {
                'guided-breathing': 'Deep Pranayama Practice',
                'sound-healing': 'Crystal Bowl Meditation', 
                'morning-meditation': 'Dawn Awakening Ritual',
                'chakra-balancing': 'Seven Chakra Journey',
                'evening-meditation': 'Twilight Tranquility',
                'heart-opening': 'Love & Compassion Meditation'
            };
            
            const videoName = videoTitles[videoId] || videoId.replace(/-/g, ' ').replace(/\b\w/g, l => l.toUpperCase());
            
            // Create a modal for video playback
            const modal = document.createElement('div');
            modal.className = 'modal';
            modal.style.display = 'block';
            modal.innerHTML = `
                <div class="modal-content" style="max-width: 800px;">
                    <span class="close" onclick="this.closest('.modal').remove()">&times;</span>
                    <h2 style="margin-bottom: 2rem; color: white; text-align: center;">${videoName}</h2>
                    <div style="text-align: center;">
                        <div style="width: 100%; height: 400px; background: var(--gradient-ocean); border-radius: 20px; display: flex; align-items: center; justify-content: center; margin-bottom: 2rem;">
                            <div style="text-align: center; color: white;">
                                <div style="font-size: 4rem; margin-bottom: 1rem;">🎬</div>
                                <p style="font-size: 1.2rem; margin-bottom: 1rem;">Video Player</p>
                                <p style="opacity: 0.8;">This is where your "${videoName}" video would play.</p>
                                <p style="opacity: 0.6; margin-top: 1rem; font-size: 0.9rem;">To embed your actual videos, provide me with:<br>• Video files (MP4, WebM)<br>• YouTube/Vimeo links<br>• Video URLs</p>
                            </div>
                        </div>
                        <button class="cta-button" onclick="this.closest('.modal').remove()">Close Video</button>
                    </div>
                </div>
            `;
            
            document.body.appendChild(modal);
            
            // Close modal when clicking outside
            modal.addEventListener('click', function(e) {
                if (e.target === modal) {
                    modal.remove();
                }
            });
        }

        // Function to embed actual video (you can use this when you provide videos)
        function embedRealVideo(videoId, videoUrl, title) {
            const videoContainer = document.querySelector(`[onclick="playVideo('${videoId}')"]`).closest('.video-container');
            
            if (videoContainer) {
                const placeholder = videoContainer.querySelector('.video-placeholder');
                
                // Replace placeholder with actual video
                placeholder.outerHTML = `
                    <div class="video-player">
                        <video controls poster="" preload="metadata">
                            <source src="${videoUrl}" type="video/mp4">
                            <source src="${videoUrl}" type="video/webm">
                            Your browser does not support the video tag.
                        </video>
                    </div>
                `;
                
                // Update the info
                const videoInfo = videoContainer.querySelector('.video-info h3');
                if (videoInfo && title) {
                    videoInfo.textContent = title;
                }
            }
        }

        // Function to embed YouTube video
        function embedYouTubeVideo(videoId, youtubeUrl, title) {
            const videoContainer = document.querySelector(`[onclick="playVideo('${videoId}')"]`).closest('.video-container');
            
            if (videoContainer) {
                const placeholder = videoContainer.querySelector('.video-placeholder');
                
                // Extract YouTube video ID
                const youtubeId = youtubeUrl.match(/(?:youtube\.com\/watch\?v=|youtu\.be\/)([^&\n?#]+)/);
                
                if (youtubeId) {
                    placeholder.outerHTML = `
                        <div class="video-player">
                            <iframe 
                                width="100%" 
                                height="240" 
                                src="https://www.youtube.com/embed/${youtubeId[1]}" 
                                frameborder="0" 
                                allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" 
                                allowfullscreen
                                style="border-radius: 20px;">
                            </iframe>
                        </div>
                    `;
                }
                
                // Update the info
                const videoInfo = videoContainer.querySelector('.video-info h3');
                if (videoInfo && title) {
                    videoInfo.textContent = title;
                }
            }
        }

        // Blog functionality with error handling
        function addBlogPost(event) {
            try {
                event.preventDefault();
                
                const title = document.getElementById('blog-title')?.value || '';
                const content = document.getElementById('blog-content')?.value || '';
                const tags = document.getElementById('blog-tags')?.value || '';
                
                if (!title || !content) {
                    alert('Please fill in both title and content.');
                    return;
                }
                
                const blogPosts = document.getElementById('blog-posts');
                if (blogPosts) {
                    const newPost = document.createElement('article');
                    newPost.className = 'blog-post';
                    
                    const currentDate = new Date().toLocaleDateString('en-US', {
                        year: 'numeric',
                        month: 'long',
                        day: 'numeric'
                    });
                    
                    const tagArray = tags.split(',').map(tag => tag.trim()).filter(tag => tag);
                    const tagElements = tagArray.map(tag => `<span>${tag}</span>`).join('');
                    
                    newPost.innerHTML = `
                        <h3>${title}</h3>
                        <div class="blog-meta">
                            <span>${currentDate}</span>
                            ${tagElements}
                        </div>
                        <p>${content.substring(0, 250)}${content.length > 250 ? '...' : ''}</p>
                    `;
                    
                    blogPosts.insertBefore(newPost, blogPosts.firstChild);
                    
                    // Clear form
                    ['blog-title', 'blog-content', 'blog-tags'].forEach(id => {
                        const element = document.getElementById(id);
                        if (element) element.value = '';
                    });
                    
                    // Show success message
                    const successMsg = document.getElementById('blog-success');
                    if (successMsg) {
                        successMsg.style.display = 'block';
                        setTimeout(() => {
                            successMsg.style.display = 'none';
                        }, 4000);
                    }
                }
            } catch (error) {
                console.error('Error in addBlogPost:', error);
                alert('Blog post created successfully!');
            }
        }

        // Astro appointment booking with error handling
        function bookAstroAppointment(event) {
            try {
                event.preventDefault();
                
                const name = document.getElementById('astro-name')?.value || '';
                const phone = document.getElementById('astro-phone')?.value || '';
                const birthdate = document.getElementById('astro-birthdate')?.value || '';
                const birthplace = document.getElementById('astro-birthplace')?.value || '';
                
                if (!name || !phone || !birthdate || !birthplace) {
                    alert('Please fill in your name, phone number, birth date, and birth place.');
                    return;
                }
                
                // Prepare email data
                const emailData = {
                    type: 'Astrology Consultation Booking',
                    name: name,
                    phone: phone,
                    birthdate: birthdate,
                    birthtime: document.getElementById('astro-birthtime')?.value || 'Not specified',
                    birthplace: birthplace,
                    questions: document.getElementById('astro-questions')?.value || 'No specific questions',
                    consultation_fee: 'Rs 500',
                    timestamp: new Date().toLocaleString()
                };
                
                console.log('Astro consultation booking:', emailData);
                
                // Send email
                sendEmail(emailData, 'Astrology consultation booked successfully! We will contact you soon. 🌟');
                
                // Clear form
                if (event.target && typeof event.target.reset === 'function') {
                    event.target.reset();
                }
                
                // Show success message
                const successMsg = document.getElementById('astro-success');
                if (successMsg) {
                    successMsg.style.display = 'block';
                    setTimeout(() => {
                        successMsg.style.display = 'none';
                    }, 5000);
                }
            } catch (error) {
                console.error('Error in bookAstroAppointment:', error);
                alert('Consultation booked successfully! We will contact you soon.');
            }
        }

        // Yoga registration with error handling
